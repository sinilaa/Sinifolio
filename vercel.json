{
    "version": 2,
    "builds": [
      {
        "src": "/api/*",
        "use": "@vercel/node"
      },
      {
        "src": "/frontend/build/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/build/index.html"
      }
    ]
  }
  