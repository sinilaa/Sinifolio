{
    "version": 2,
    "builds": [
        {
            "src": "client/**/*.js",
            "use": "@vercel/static-build" 
        {
            "src": "api/**/*.js", 
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "client/public/$1"
        }
    ]
}
